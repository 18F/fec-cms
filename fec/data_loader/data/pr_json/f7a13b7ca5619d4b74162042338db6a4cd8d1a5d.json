tr>\n        <th width=\"5%\" valign=\"top\" align=\"left\"/>\n        <th width=\"26%\" valign=\"top\" align=\"left\"/>\n        <td width=\"69%\" valign=\"top\"/>\n      </tr>\n      <tr>\n        <th width=\"5%\" valign=\"top\" align=\"left\">6.</th>\n        <th width=\"26%\" valign=\"top\" align=\"left\">AF 0528</th>\n        <td width=\"69%\" valign=\"top\"/>\n      </tr>\n      <tr>\n        <th width=\"5%\" valign=\"top\" align=\"left\"/>\n        <th width=\"26%\" valign=\"top\" align=\"left\"/>\n        <td width=\"69%\" valign=\"top\"/>\n      </tr>\n      <tr>\n        <th width=\"5%\" valign=\"top\" align=\"left\"/>\n        <th width=\"26%\" valign=\"top\" align=\"left\">RESPONDENTS:</th>\n        <td width=\"69%\" valign=\"top\">Committee on Letter Carriers Political Education (Letter\n        Carriers Political Action Fund), Florence Johnson, treasurer</td>\n      </tr>\n      <tr>\n        <th width=\"5%\" valign=\"top\" align=\"left\"/>\n        <th width=\"26%\" valign=\"top\" align=\"left\">REPORT:</th>\n        <td width=\"69%\" valign=\"top\">September Monthly 2001</td>\n      </tr>\n      <tr>\n        <th width=\"5%\" valign=\"top\" align=\"left\"/>\n        <th width=\"26%\" valign=\"top\" align=\"left\">* LATE/NON-FILER:</th>\n        <td width=\"69%\" valign=\"top\">Non-Filers</td>\n      </tr>\n      <tr>\n        <th width=\"5%\" valign=\"top\" align=\"left\"/>\n        <th width=\"26%\" valign=\"top\" align=\"left\">PENALTY:</th>\n        <td width=\"69%\" valign=\"top\">$4,500 civil money penalty</td>\n      </tr>\n      <tr>\n        <th width=\"5%\" valign=\"top\" align=\"left\"/>\n        <th width=\"26%\" valign=\"top\" align=\"left\"/>\n        <td width=\"69%\" valign=\"top\"/>\n      </tr>\n      <tr>\n        <th width=\"5%\" valign=\"top\" align=\"left\">7.</th>\n        <th width=\"26%\" valign=\"top\" align=\"left\">AF 0529</th>\n        <td width=\"69%\" valign=\"top\"/>\n      </tr>\n      <tr>\n        <th width=\"5%\" va